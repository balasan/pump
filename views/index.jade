#pageMenu.uiClosed(name='pageMenu', style='left:400px; top:300px;')
  form(name='privacy')
    | Set Page Privacy:
    br
    -var check = false
    -if (privacy==0)
      -check=true
    input(type='radio', name='privacy', value='0',checked=check)
    | public
    br
    -var check = false
    -if (privacy==1)
      -check=true
    input(type='radio', name='privacy', value='1',checked=check)
    | only i can add/remove images
    br
    -var check = false
    -if (privacy==2)
      -check=true
    input(type='radio', name='privacy', value='2',checked=check)
    | only i can make changes
    br
    -var check = false
    -if (privacy==3)
      -check=true
    input(type='radio', name='privacy', value='3',checked=check)
    | only i can see
    br
  input(style='width:160px;', type='text',id='addEditors', name='addEditors', placeholder='enter user name', onfocus="value=''", onkeyup="searchUsers()")
  #searchResults
  input#pageSettings.button(type='submit', style='width:78px;', name='pageSettings', value='Save',onclick='setPrivacy()') 
  | 
  input#deletePage.button(type='submit', style='width:78px;', name='deletePage', value='Delete Page',onclick='deletePage()')
